#include<stdio.h>
int main(){

    // prob A --> Number Mirror
	  int x;
	  scanf("%d", &x);
	  printf("%d", x);
    
    // prob B --> Is It A Vowel or Consonant
	  char j;
	  scanf("%c", &j);
	
	  if ((j == 'A') || (j == 'I') || (j == 'U') || (j == 'E') || (j == 'O')){
		  printf("Vowel");
	  } else {
		  printf("Consonant");
	  }
    
    // prob C --> Area or Perimeter
	  int lenght, breadth, peri, area;
	
	  scanf("%d%d", &lenght, &breadth);
	
	  area = lenght*breadth;
	  peri = 2 * (lenght + breadth);
	
	  if (area>peri){
		printf("Area\n");
		printf("%d", area);
	  }else if (peri>area){
		printf("Peri\n");
		printf("%d", peri);
	  }else{
		printf("Eq\n");
		printf("%d", area);
	  }
    
   	// prob D --> How Many Digits Do I Have
	  int y;
		scanf ("%d", &y);
		if ( y >= 0 && y <= 9){
			printf ("1");
		}
		else if ( y >= 10 && y <= 99){
			printf ("2");
		}
		else if ( y >= 100 && y <= 999){
			printf ("3");
		}
		else if ( y >= 1000 && y <= 1000000){
			printf ("More than 3 digits");
		}
    
    // prob E --> Sum Or Difference
    int n1, n2;
	  scanf("%d%d", &n1, &n2);
	  if (n1 > n2){
		  printf("%d\n", n1 - n2);
	  } else {
		  printf("%d\n", n1 + n2);
	  }
    
    
    
